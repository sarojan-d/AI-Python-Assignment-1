    "#Write a program to check palindrome.\n",
    "\n",
    "ori = input(\"Enter string: \")\n",
    "\n",
    "def pali(str):\n",
    "    rev= str[::-1]\n",
    "    if str==rev:\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "\n",
    "if pali(ori):\n",
    "    print(f'{ori}  is a palindrome.')\n",
    "else:\n",
    "    print(f'{ori}  is not a palindrome.')"
   ]
  }
